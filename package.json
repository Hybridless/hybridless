{
  "name": "@hybridless/hybridless",
  "version": "0.0.143",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf ./*.js ./options.json ./*.js.map ./bin/ ./.serverless/ ./resources/ ./core/",
    "generate-plugin-schema": "typescript-json-schema --required --noExtraProps src/options.ts '*' > options.json && node config/export-plugin-schema.js",
    "build": "npm run clean && npm run generate-plugin-schema && tsc -p ./tsconfig.json && npm run copy:assets",
    "test": "echo \"Error: no test specified\" && exit 1",
    "copy:assets": "cp -R ./src/assets ./resources/assets"
  },
  "license": "GNU GPLv3",
  "devDependencies": {
    "@types/node": "^20.5.6",
    "typescript": "^5.2.2",
    "typescript-json-schema": "^0.60.0"
  },
  "dependencies": {
    "@babel/core": "^7.22.11",
    "@hybridless/serverless-ecs-plugin": "0.0.33",
    "babel-loader": "^9.1.3",
    "bluebird": "^3.7.2",
    "dockerode": "^3.3.5",
    "es6-promise-pool": "^2.5.0",
    "json-schema-ref-parser": "^9.0.9",
    "json-stringify-safe": "^5.0.1",
    "loadash": "^1.0.0",
    "serverless-plugin-log-retention": "^2.0.0",
    "serverless-webpack": "^5.13.0",
    "stack-trace": "0.0.10",
    "tar-fs": "^3.0.4",
    "uuid": "^9.0.0",
    "webpack": "^5.88.2",
    "webpack-node-externals": "^3.0.0"
  }
}
