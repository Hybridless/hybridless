{
  "name": "@hybridless/hybridless",
  "version": "0.0.7",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf ./*.js ./options.json ./*.js.map ./bin/ ./.serverless/ ./resources/ ./core/",
    "generate-plugin-schema": "typescript-json-schema --required --noExtraProps src/options.ts '*' > options.json && node config/export-plugin-schema.js",
    "build": "npm run clean && npm run generate-plugin-schema && tsc -p ./tsconfig.json && npm run copy:assets",
    "test": "echo \"Error: no test specified\" && exit 1",
    "copy:assets": "cp -R ./src/assets ./resources/assets"
  },
  "license": "GNU GPLv3",
  "devDependencies": {
    "@types/node": "^15.0.3",
    "typescript": "^4.2.4",
    "typescript-json-schema": "^0.50.0"
  },
  "dependencies": {
    "@babel/core": "^7.14.2",
    "@hybridless/serverless-ecs-plugin": "0.0.5",
    "babel-loader": "^8.2.2",
    "bluebird": "^3.7.2",
    "dockerode": "^3.3.0",
    "json-schema-ref-parser": "^9.0.7",
    "json-stringify-safe": "^5.0.1",
    "loadash": "^1.0.0",
    "serverless-webpack": "^5.5.0",
    "stack-trace": "0.0.10",
    "tar-fs": "^2.1.1",
    "uuid": "^8.3.2",
    "webpack": "^5.37.0",
    "webpack-node-externals": "^3.0.0"
  }
}
